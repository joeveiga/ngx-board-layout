{
  "rulesDirectory": ["node_modules/codelyzer"],
  "extends": ["codelyzer", "tslint-config-prettier"],
  "rules": {
    "no-implicit-dependencies": [false, "dev"],
    "deprecation": {
      "severity": "warning"
    },
    "no-any": false,
    "typedef": true,
    "no-floating-promises": false,
    "array-type": [true, "array-simple"]
  }
}
