{
  "compileOnSave": false,
  "compilerOptions": {
    "rootDir": ".",
    "baseUrl": "./",
    "importHelpers": true,
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "module": "es2020",
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "target": "es2015",
    "typeRoots": ["node_modules/@types"],
    "lib": ["es2018", "dom"],
    "paths": {
      "ngx-board-layout": ["projects/ngx-board-layout/src/public_api"],
      "ngx-board-layout/*": ["projects/ngx-board-layout/src/lib/*"],
      "@angular/*": ["node_modules/@angular/*"],
      "tslib": ["node_modules/tslib"]
    },
    "allowSyntheticDefaultImports": true
  }
}
